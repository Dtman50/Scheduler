<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.softwareii_dt.controller.GatewayController">
    <Label layoutX="187.0" layoutY="69.0" text="Welcome To MyOffice">
        <font>
          <Font name="Times New Roman" size="24.0" />
        </font>
    </Label>
    <Label layoutX="195.0" layoutY="131.0" text="Select The Service You Want To Use">
        <font>
          <Font name="Times New Roman" size="14.0" />
        </font>
    </Label>
    <Button fx:id="viewCustomersBtn" layoutX="38.0" layoutY="184.0" mnemonicParsing="false" onAction="#toRecords" prefHeight="81.0" prefWidth="149.0" text="View Customer Records" />
    <Button fx:id="viewSchedulerBtn" layoutX="227.0" layoutY="184.0" mnemonicParsing="false" onAction="#toScheduler" prefHeight="81.0" prefWidth="148.0" text="Schedule Appointments" />
    <Button fx:id="exitAppBtn" layoutX="281.0" layoutY="311.0" mnemonicParsing="false" onAction="#exit" text="Exit " />
   <Button layoutX="413.0" layoutY="184.0" mnemonicParsing="false" onAction="#toReports" prefHeight="81.0" prefWidth="148.0" text="View Reports" />
</AnchorPane>
